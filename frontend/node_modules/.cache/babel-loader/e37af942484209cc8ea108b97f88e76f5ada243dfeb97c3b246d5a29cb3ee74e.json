j){\r\n            for (const key in obj){\r\n                if(Object.prototype.hasOwnProperty.call(obj, key)){\r\n                    if(obj[key] === ''){\r\n                        return true;\r\n                    }\r\n                }\r\n            }\r\n            return false;\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n        .box {\r\n            width: 350px;\r\n            height: 300px;\r\n        }\r\n        form {\r\n            max-width: 400px;\r\n            margin: 0 auto;\r\n            \r\n        }\r\n\r\n        label {\r\n            display: block;\r\n            margin-bottom: 5px;\r\n            font-weight: bold;\r\n        }\r\n\r\n        select,\r\n        input[type=\"number\"] {\r\n            width: 100%;\r\n            padding: 5px;\r\n            margin-bottom: 10px;\r\n        }\r\n\r\n        input[type=\"submit\"] {\r\n            background-color: #4CAF50;\r\n            color: white;\r\n            padding: 10px 15px;\r\n            border: none;\r\n            cursor: pointer;\r\n            align-self: center;\r\n            margin-top: 20px;\r\n            width: 100%;\r\n        }\r\n        h1 {\r\n            text-align: center;\r\n        }\r\n        \r\n</style>"],"mappings":"AA+FA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,IAAA;QACAC,QAAA;QACAC,QAAA;QACAC,KAAA;QACAC,UAAA;QACAC,YAAA;QACAC,KAAA;QACAC,YAAA;QACAC,QAAA;QACAC,WAAA;QACAC,SAAA;QACAC,SAAA;QACAC,OAAA;QACAC,QAAA;MACA;MACAC,QAAA;MACAC,YAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,SAAAC,aAAA,MAAAnB,IAAA;QACAoB,KAAA;MACA;QACA,KAAAN,QAAA;QACAhB,KAAA,CAAAuB,IAAA,kBAAArB,IAAA,EACAsB,IAAA,CAAAC,GAAA;UACA,KAAAR,YAAA,GAAAQ,GAAA,CAAAxB,IAAA,CAAAyB,KAAA;UACA,KAAAV,QAAA;UACA,KAAAE,SAAA;QACA,GACAS,KAAA,CAAAC,GAAA;UACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QAEA;MACA;IACA;IACAP,cAAAU,GAAA;MACA,WAAAC,GAAA,IAAAD,GAAA;QACA,IAAAE,MAAA,CAAAC,SAAA,CAAAC,cAAA,CAAAC,IAAA,CAAAL,GAAA,EAAAC,GAAA;UACA,IAAAD,GAAA,CAAAC,GAAA;YACA;UACA;QACA;MACA;MACA;IACA;EACA;AAEA"}